<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.caRing.repository.HostMapper">
    <insert id="saveHost">
        insert into Host (
            host_email,
            host_password,
            host_name,
            host_phone
        ) values (
            #{host_email},
            #{host_password},
            #{host_name},
            #{host_phone}
        )
    </insert>
</mapper>